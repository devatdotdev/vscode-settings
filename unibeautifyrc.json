{
  "HTML": {
    "beautifiers": [
      "Pretty Diff", "JS-Beautify"
    ],
    "align_assignments": true,
    "brace_style": "collapse",
    "break_chained_methods": true,
    "end_with_comma": false,
    "end_with_newline": true,
    "force_indentation": true,
    "indent_comments": true,
    "indent_inner_html": true,
    "indent_scripts": "keep",
    "indent_size": 2,
    "indent_style": "space",
    "max_preserve_newlines": 1,
    "multiline_ternary": "always-multiline",
    "newline_before_tags": [
      "section",
      "main",
      "aside",
      "header",
      "footer",
      "script",
      "style"
    ],
    "preserve_newlines": false,
    "quotes": "double",
    "space_after_anon_function": true,
    "unformatted": [
      "script", "style"
    ],
    "wrap_attributes": "force-aligned",
    "wrap_attributes_indent_size": 2,
    "wrap_line_length": 80
  },
  "CSS": {
    "beautifiers": [
      "Prettier", "Pretty Diff", "JS-Beautify"
    ],
    "align_assignments": true,
    "break_chained_methods": true,
    "end_with_comma": false,
    "end_with_newline": true,
    "end_with_semicolon": true,
    "indent_comments": true,
    "indent_size": 2,
    "indent_style": "space",
    "max_preserve_newlines": 1,
    "multiline_ternary": "always-multiline",
    "newline_between_rules": true,
    "no_leading_zero": true,
    "pragma_insert": true,
    "pragma_require": true,
    "preserve_newlines": false,
    "quotes": "double",
    "selector_separator_newline": true,
    "space_after_anon_function": true,
    "wrap_line_length": 80
  },
  "JavaScript": {
    "beautifiers": ["Pretty Diff"],
    "align_assignments": true,
    "arrow_parens": "always",
    "brace_style": "end-expand",
    "break_chained_methods": true,
    "comma_first": false,
    "end_with_comma": false,
    "end_with_newline": true,
    "end_with_semicolon": true,
    "indent_chained_methods": true,
    "indent_comments": true,
    "indent_size": 2,
    "indent_style": "space",
    "jslint_happy": true,
    "jsx_brackets": true,
    "keep_array_indentation": false,
    "max_preserve_newlines": 1,
    "multiline_ternary": "always-multiline",
    "object_curly_spacing": true,
    "pragma_insert": true,
    "pragma_require": true,
    "preserve_newlines": false,
    "quotes": "double",
    "remove_trailing_whitespace": true,
    "space_after_anon_function": false,
    "space_before_conditional": false,
    "space_in_empty_paren": false,
    "space_in_paren": false,
    "unescape_strings": false,
    "unindent_chained_methods": false
  },
  "Liquid": {
    "beautifiers": ["Pretty Diff"],
    "brace_style": "expand",
    "end_with_newline": true,
    "indent_inner_html": true,
    "indent_scripts": "keep",
    "indent_size": 2,
    "indent_style": "space",
    "max_preserve_newlines": 1,
    "newline_before_tags": [
      "script",
      "style",
      "header",
      "section",
      "footer",
      "main",
      "aside",
      "div",
      "schema"
    ],
    "preserve_newlines": false,
    "unformatted": [
      "script", "style"
    ],
    "wrap_attributes": "force-aligned",
    "wrap_attributes_indent_size": 2,
    "wrap_line_length": 80
  },
  "Sass": {
    "beautifiers": ["Pretty Diff"],
    "align_assignments": true,
    "break_chained_methods": true,
    "end_with_comma": false,
    "indent_comments": true,
    "indent_size": 2,
    "indent_style": "space",
    "max_preserve_newlines": 1,
    "multiline_ternary": "always-multiline",
    "newline_between_rules": true,
    "no_leading_zero": true,
    "quotes": "double",
    "space_after_anon_function": true,
    "wrap_line_length": 80
  },
  "SCSS": {
    "beautifiers": [
      "Prettier", "Pretty Diff"
    ],
    "align_assignments": true,
    "break_chained_methods": true,
    "end_with_comma": false,
    "end_with_semicolon": true,
    "indent_comments": true,
    "indent_size": 2,
    "indent_style": "space",
    "max_preserve_newlines": 1,
    "multiline_ternary": "always-multiline",
    "newline_between_rules": true,
    "no_leading_zero": true,
    "pragma_insert": true,
    "pragma_require": true,
    "quotes": "double",
    "space_after_anon_function": true,
    "wrap_line_length": 80
  },
  "JSON": {
    "beautifiers": [
      "Prettier", "Pretty Diff", "JS-Beautify"
    ],
    "align_assignments": true,
    "arrow_parens": "as-needed",
    "brace_style": "collapse",
    "break_chained_methods": true,
    "comma_first": false,
    "end_with_comma": false,
    "end_with_newline": true,
    "end_with_semicolon": false,
    "indent_comments": true,
    "indent_size": 2,
    "indent_style": "space",
    "jslint_happy": true,
    "jsx_brackets": true,
    "keep_array_indentation": false,
    "max_preserve_newlines": 1,
    "multiline_ternary": "always-multiline",
    "newline_between_rules": false,
    "no_leading_zero": true,
    "object_curly_spacing": true,
    "pragma_insert": true,
    "pragma_require": true,
    "preserve_newlines": false,
    "quotes": "double",
    "space_after_anon_function": true,
    "space_before_conditional": true,
    "space_in_empty_paren": true,
    "space_in_paren": true,
    "unescape_strings": false,
    "unindent_chained_methods": false,
    "wrap_line_length": 80
  }
}